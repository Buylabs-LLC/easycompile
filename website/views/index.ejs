<%- include('partials/header.ejs') %>
<div id="wrapper">
    <div id="main">
        <div class="inner">
            <h1 style="font-size: 44px; color: #ffffff;">EasyCompile</h1>
            <form enctype="multipart/form-data" id="form01" method="post" action="/sendform" autocomplete="off"
                data-gramm="false">
                <div class="inner">
                    <div class="field">
                        <h1 style="float: left; color: #ffffff; margin-bottom: .5vw;">File Name:</h1>
                        <input type="text" name="name" id="" maxlength="256" required="" placeholder="File name..."
                            style="font-family: 'Asap', sans-serif; color: #ffffff;">
                    </div>
                    <div class="field">
                        <h1 style="float: left; color: #ffffff; margin-bottom: .5vw;">JavaScript Code:</h1>
                        <textarea name="content" id="" rows="10" placeholder="Place JavaScript code here..."></textarea>
                    </div>
                    <div class="actions">
                        <button type="submit">Create File</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
<%- include('partials/footer.ejs') %>